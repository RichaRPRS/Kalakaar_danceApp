<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kalakaar.admin.danceapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherkalak"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcherkalak_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".CategoryActivity" />
        <activity android:name=".AddpostActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".AboutusActivity" />
        <activity android:name=".ContactusActivity" />
        <activity android:name=".RegistraAdminActivity" />
        <activity android:name=".PostsdetailsActivity" />
        <activity android:name=".PostDetailsActivity">
            <intent-filter>
                <action android:name=".PostDetailsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryDetailsActivity" />
        <activity android:name=".AppliedApplicationActivity">
            <intent-filter>
                <action android:name=".AppliedApplicationActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".UserEditActivity" />
        <activity android:name=".ProvidEditActivity" />
        <activity android:name=".LoginadminActivity" />
        <activity android:name=".AdminpostActivity" />
        <activity android:name=".AdminpostdetailActivity" />
        <activity android:name=".AdmineditActivity" />

        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".FullScreenViewActivity" />
    </application>

</manifest>